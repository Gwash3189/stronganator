{
  "name": "stronganator",
  "version": "0.0.1",
  "description": "Types and (optional) run time type checking for JavaScript",
  "main": "dist/stronganator.js",
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "lodash": "~3.10.1"
  },
  "devDependencies": {
    "babel": "~5.8.23",
    "babel-cli": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "~3.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-mocha": "^1.1.0",
    "istanbul": "~0.3.22",
    "mocha": "~2.3.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sinon": "^1.17.2"
  },
  "scripts": {
    "build": "babel src/stronganator.js > dist/stronganator.js",
    "build:watch": "babel --watch --out-file dist/stronganator.js src/**/*.js",
    "test": "./node_modules/.bin/mocha 'test/**/*.js' --compilers js:babel-register",
    "test:watch": "./node_modules/.bin/mocha 'test/**/*.js' --compilers js:babel-register --watch",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec 'test/**/*.js' --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gwash3189/stronganator.git"
  },
  "keywords": [
    "type",
    "checking",
    "types",
    "strong",
    "run",
    "time"
  ],
  "author": "Adam Beck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gwash3189/stronganator/issues"
  }
}
